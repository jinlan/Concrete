<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="StoneBlocksBase">
    <defName>Concrete</defName>
    <label>Concrete</label>
    <description>Concrete. As hard as other stony material, fast to build, but require days to hydrate.</description>
    <tickerType>Rare</tickerType>
    <graphicData>
      <texPath>Concrete</texPath>
    </graphicData>
    <statBases>
      <Mass>1</Mass>
    </statBases>
    <stuffProps>
      <soundImpactStuff>BulletImpactGround</soundImpactStuff>
      <soundMeleeHitSharp>MeleeHit_Stone</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Stone</soundMeleeHitBlunt>
      <stuffAdjective>concrete</stuffAdjective>
      <statFactors>
        <WorkToBuild>0.5</WorkToBuild>
        <Beauty>0</Beauty>
        <MaxHitPoints>2.3</MaxHitPoints>
       <Flammability>0</Flammability>
      </statFactors>
    </stuffProps>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>0.05</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>
</Defs>